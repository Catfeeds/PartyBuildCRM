.indexImg {
  background-color: #E6E4E5;
  width: 100%;
  height: 580rpx;
}
.indexImg image {
  display: inline-block;
  width: 100%;
  height: 100%;
}
/* content  */
.fl {
  float: left
}
.fr {
  float: right
}
.clearfix::before {
  display: table;
  content: '';
}
.clearfix::after {
  display: table;
  content: '';
  clear: both;
}
.content {
  margin-bottom: 98rpx;
}
.contentTitle {
  padding: 26rpx 30rpx 29rpx;
  background-color: #fff;
}
.contentTitle .title {
  line-height: 54rpx;
  color: #333;
  font-size: 34rpx;
  font-family: 'PingFang SC' 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  font-weight: Medium;
  margin-bottom: 30rpx;
}
.contentTitle .priceShare {
  position: relative;
}
.contentTitle .price {
  color: #C71B1B;
  font-size: 26rpx;
  font-family: 'PingFang-SC-Medium' 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  font-weight: Medium;
}
.contentTitle .price text {
  font-size: 42rpx;
  font-weight: bold;
}
.contentTitle .share {
  color: #666;
  font-family: 'PingFang-SC-Regular' 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  font-weight: Regular;
  font-size: 24rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}
.line {
  width: 100%;
  height: 10rpx;
}
.rangeCase,.productContent {
  padding: 15rpx 30rpx 34rpx;
  background-color: #fff
}
.common {
  color: #666;
  font-size: 28rpx;
  line-height: 48rpx;
}
.common text {
  color: #999;
}

/* btn */
.btn {
  position: fixed;
  width: 100%;
  height: 98rpx;
  background-color: #fff;
  box-shadow: -1rpx -1rpx 1rpx rgba(150,150,150,0.2) ;
  bottom: 0;
  left: 0;
}
.btn .collect {
  width: 120rpx;
  height: 100%;
  display: inline-flex;
}
.btn .collect navigator {
  display: inline-block;
  width: 100%;
  height: 100%;
}
.btn .collect navigator image {
  width: 30rpx;
  height: 30rpx;
  margin: 19rpx auto 14rpx;
  display: block
}
.btn .collect navigator text {
  color: #666;
  font-size: 20rpx;
  display: block;
  text-align: center;
}
.btn .fr {
  margin-top: 10rpx;
  margin-right: 30rpx;
}
.btn .status1 navigator {
  background: -webkit-linear-gradient(left, #C71B1B , #ED5858); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #C71B1B, #ED5858); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #C71B1B, #ED5858); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #C71B1B , #ED5858); /* 标准的语法 */
  border-radius: 6.7vw;
  font-size: 30rpx;
  font-weight: bold;
  padding: 21rpx 199rpx;
  color: #fff;
}
.btn .status2 navigator:first-of-type {
  background: -webkit-linear-gradient(left, #FFC500 , #F7DB7A); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #FFC500, #F7DB7A); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #FFC500, #F7DB7A); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #FFC500 , #F7DB7A); /* 标准的语法 */
  border-radius: 6.7vw 0 0 6.7vw;
  padding: 21rpx 73rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #fff;
  display: inline-block
}
.btn .status2 navigator:last-of-type {
  background: -webkit-linear-gradient(left, #C71B1B , #ED5858); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #C71B1B, #ED5858); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #C71B1B, #ED5858); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #C71B1B , #ED5858); /* 标准的语法 */
  border-radius: 0 6.7vw 6.7vw 0;
  padding: 21rpx 60rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #fff;
  display: inline-block
}